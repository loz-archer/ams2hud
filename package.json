{
  "name": "ams2.skin--hud",
  "version": "1.0.6",
  "description": "A custom HUD for AMS2",
  "main": "./out/main/index.js",
  "author": {
    "name": "Laurence Archer",
    "email": "laurence@weareindi.co.uk",
    "url": "https://weareindi.co.uk"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "npm run postinstall && electron-vite preview",
    "dev": "electron-vite dev",
    "watch": "electron-vite dev --watch",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run postinstall && npm run build && electron-builder --win --config",
    "build:mac": "npm run postinstall && npm run build && electron-builder --mac --config",
    "build:linux": "npm run postinstall && npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
